{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information",
      "MQTTBrokerService": "Information"
    }
  },
  "MqttBroker": {
    "Port": 1883,
    "MaxPendingConnections": 100,
    "EnableAuthentication": false,
    "EnableVerboseLogging": false,
    "CommunicationTimeout": 30,
    "AutoStart": true
  },
  "Urls": "http://localhost:5000"
}
